class Car {
    String color;
    int speed;

    void drive() {
        System.out.println("Car is driving...");
    }
}

public class ClassesObjects {
    public static void main(String[] args) {

        Car myCar = new Car();
        myCar.color = "Red";
        myCar.speed = 100;

        System.out.println(myCar.color);
        myCar.drive();
    }
}
